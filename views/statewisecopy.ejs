    <div class="state py-4 no-gutter ">
        <div class="container">
            <div class="heading">
                <p><small class="lead">ATMANIRBHAR STATES !!</small></p>
                <h2>STATEWISE WISE REPORT</h2>
                <a href="#" class="d-none d-sm-inline-block btn btn-sm btn-info shadow-sm"><i class="fa fa-pencil" aria-hidden="true"></i>
                  Last Update <%=states[0].lastupdatedtime%></a>
            </div>
            
    <div id="container"></div>
        
            <table class="table table-hover table-responsive-lg">
                <thead>
                <tr>
                  <th scope="col">STATE</th>
                  <th scope="col">CONFIRMED</th>
                  <th scope="col">RECOVERED</th>
                  <th scope="col">DECEASED</th>
                </tr>
               </thead>
               <!-- {
                "active": "53832",
                "confirmed": "91315",
                "deaths": "2897",
                "deltaconfirmed": "666",
                "deltadeaths": "25",
                "deltarecovered": "324",
