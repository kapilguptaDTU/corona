    <div class="state py-4 no-gutter ">
        <div class="container">
            <div class="heading">
                <p><small class="lead">ATMANIRBHAR EVENTS !!</small></p>
                <h2>EVENT WISE REPORT</h2>
                <a href="#" class="d-none d-sm-inline-block btn btn-sm btn-info shadow-sm"><i class="fa fa-pencil" aria-hidden="true"></i>
                  Last Update 1 HOUR AGO</a>
            </div>
            <table class="table table-hover table-responsive-lg">
                <thead>
                <tr>
                  <th scope="col">VENUE</th>
                  <th scope="col">CONF NAME</th>
                  <th scope="col">CONF START DATE</th>
                  <th scope="col">CITY</th>
                </tr>
               </thead>
              
               <!-- <th scope="col">VENUE</th>
               <th scope="col">CONF NAME</th>
               <th scope="col">CONF START DATE</th>
               <th scope="col">CITY</th> -->


                <tbody>
                    <% events.forEach(function(event){ %>
                <tr>
                  <th scope="row"><%=event.venue%></th>
                  <td>-<%=event.confName%>
                  </td>
                  <td><%=event.confStartDate%>
                  </td>
                  <td><%=event.confEndDate%>
                  </td>
                </tr>
                
               <%})%>
               </tbody>
            </table>

        </div>
    </div>




